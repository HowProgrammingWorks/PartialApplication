[{
  name: 'power',
  length: [32, 32],
  cases: [
    [0, 0, 1],
    [1, 1, 1],
    [1, 2, 1],
    [2, 1, 2],
    [2, 2, 4],
    [3, 1, 3],
    [3, 2, 9],
    [3, 3, 27],
    [1, -1, 1],
    [1, -2, 1],
    [2, -1, 0.5],
    [2, -2, 0.25],
  ],
  test: power => {
    const src = power.toString();
    if (src !== 'function pow() { [native code] }') {
      throw new Error('Function power expected to be reference to Math.pow');
    }
  }
}, {
  name: 'square',
  length: [10, 25],
  cases: [
    [5, 25],
    [6, 36],
    [9, 81],
  ]
}, {
  name: 'powerOfThree',
  length: [29, 29],
  test: powerOfThree => {
    const src = powerOfThree.toString();
    if (src !== 'function () { [native code] }') {
      throw new Error('Function powerOfThree expected to be binded Math.pow');
    }
  },
  cases: [
    [2, 9],
    [3, 27],
    [4, 81],
  ]
}]
